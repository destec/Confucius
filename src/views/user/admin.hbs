<div class="pageHeader">
	<form onsubmit="return navTabSearch(this);" action="" method="post">
	<div class="searchBar">
		<table class="searchContent">
			<tr>
				<td>
					我的客户：<input type="text" name="keyword" />
				</td>
				<td>
					<select class="combox" name="province">
						<option value="">所有省市</option>
						<option value="北京">北京</option>
						<option value="上海">上海</option>
						<option value="天津">天津</option>
						<option value="重庆">重庆</option>
						<option value="广东">广东</option>
					</select>
				</td>
				<td>
					建档日期：<input type="text" class="date" readonly="true" />
				</td>
			</tr>
		</table>
		<div class="subBar">
			<ul>
				<li><div class="buttonActive"><div class="buttonContent"><button type="submit">检索</button></div></div></li>
				<li><a class="button" href="demo_page6.html" target="dialog" mask="true" title="查询框"><span>高级检索</span></a></li>
			</ul>
		</div>
	</div>
	</form>
</div>
<div class="pageContent">
	<div class="panelBar">
		<ul class="toolBar">
			<li><a class="add" href="/user/admins/create" target="dialog" rel="admin" mask="true" title="添加"><span>添加</span></a></li>
			<li><a class="delete" href="/user/admins/delete?uid={sid_user}" target="ajaxTodo" title="确定要删除吗?"><span>删除</span></a></li>
			<li><a class="edit" href="/user/admins/edit?uid={sid_user}" target="dialog" rel="admin" mask="true" title="修改"><span>修改</span></a></li>
		</ul>
	</div>
	<table class="table" width="100%" layoutH="138">
		<thead>
			<tr>
				<th width="120">管理员编号</th>
				<th>管理员账号</th>
				<th>管理员姓名</th>
				<th width="100">管理员邮件</th>
				<th width="80">创建时间</th>
				<th width="80">更新时间</th>
			</tr>
		</thead>
		<tbody>
			{{#each users}}
				<tr target="sid_user" rel={{id}}>
					<td>{{code}}</td>
					<td>{{username}}</td>
					<td>{{realname}}</td>
					<td>{{email}}</td>
					<td>{{createdAt}}</td>
					<td>{{updatedAt}}</td>
				</tr>
			{{/each}}
		</tbody>
	</table>
	<div class="panelBar">
		<div class="pages">
			<span>共{{count}}条</span>
		</div>

		<div class="pagination" targetType="navTab" totalCount={{count}} numPerPage="20" pageNumShown="10" currentPage="1"></div>

	</div>
</div>
