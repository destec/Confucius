
<div class="pageContent">
	<form method="post" action="/user/admins/create" class="pageForm required-validate" onsubmit="return validateCallback(this, dialogAjaxDone);">
		<div class="pageFormContent" layoutH="56">
			<p>
				<label>教师编号: </label>
				<input name="code" type="text" size="30" alt="编号需唯一"/>
			</p>
			<p>
				<label>教师真实姓名: </label>
				<input name="realname" type="text" size="30" alt="输入教师真实姓名"/>
			</p>
			<p>
				<label>教师账号: </label>
				<input name="username" class="required" type="text" size="30" alt="请输入教师账号"/>
			</p>
			<p>
				<label>教师密码: </label>
				<input name="password" class="required password" type="password" size="30" alt="请输入教师密码"/>
			</p>
			<p>
				<label>教师邮件: </label>
				<input name="email" class="email" size="30" alt="请输入管理员邮件" />
			</p>
		</div>
		<div class="formBar">
			<ul>
				<li><div class="buttonActive"><div class="buttonContent"><button type="submit">保存</button></div></div></li>
				<li>
					<div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div>
				</li>
			</ul>
		</div>
	</form>
</div>
